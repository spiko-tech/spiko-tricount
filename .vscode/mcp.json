{
  "servers": {
    "nx-mcp": {
      "command": "npx",
      "args": ["nx-mcp@latest", "."]
    },
    "effect-docs": {
      "command": "npx",
      "args": ["-y", "effect-mcp@latest"]
    },
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"]
    },
    "chrome-devtools": {
      "command": "npx",
      "args": ["-y", "chrome-devtools-mcp@latest"]
    },
    "shadcn": {
      "command": "npx",
      "args": ["shadcn@latest", "mcp"]
    }
  }
}
